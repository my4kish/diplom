{
  "$schema": "https://cdn.statically.io/gh/nguyenngoclongdev/cdn/main/schema/v10/terminal-keeper.json",
  "theme": "tribe",
  "active": "default",
  "activateOnStartup": true,
  "keepExistingTerminals": false,
  "sessions": {
    "default": [
      {
        "name": "git",
        "icon": "github",
        "color": "terminal.ansiYellow",
        "commands": [
          "git status"
        ]
      },
      {
        "name": "prisma",
        "icon": "database",
        "color": "terminal.ansiGreen",
        "cwd": "backend",
        "commands": [
          "npx prisma studio"
        ]
      },
      [
        {
          "name": "client",
          "icon": "person",
          "color": "terminal.ansiGreen",
          "cwd": "frontend",
          "commands": [
            "ng serve"
          ]
        },
        {
          "name": "client change",
          "icon": "person",
          "color": "terminal.ansiWhite",
          "cwd": "frontend",
          "commands": []
        }
      ],
      [
        {
          "name": "server",
          "icon": "server",
          "color": "terminal.ansiGreen",
          "cwd": "backend",
          "commands": [
            "npm run start:dev"
          ]
        },
        {
          "name": "server change",
          "icon": "server",
          "color": "terminal.ansiWhite",
          "cwd": "backend",
          "commands": []
        }
      ]
    ],
    "saved-session": [
      {
        "name": "connect",
        "commands": [
          ""
        ]
      }
    ]
  }
}